{
  "metadata": {
    "name": "AI 会议纪要",
    "title": "AI 会议纪要",
    "description": "实时会议转写，AI 提炼总结，一键同步任务！",
    "images": {
      "logoLight": "/logo-light.svg",
      "logoDark": "/logo-dark.svg",
      "ogImage": "/og-image.svg"
    },
    "keywords": [
      "AI 总结",
      "录音转文字",
      "同步任务"
    ]
  },
  "ui": {
    "theme": {
      "enabled": true,
      "defaultMode": "system"
    }
  },
  "i18n": {
    "enabled": true,
    "defaultLocale": "en",
    "locales": {
      "en": {
        "name": "English"
      },
      "zh": {
        "name": "简体中文"
      }
    },
    "localeCookieName": "NEXT_LOCALE"
  },
  "auth": {
    "enableSocialLogin": true,
    "enablePasswordLogin": true,
    "redirectAfterSignIn": "/app/dashboard",
    "redirectAfterLogout": "/"
  },
  "settings": {
    "account": {
      "canChangeEmail": true
    }
  },
  "payment": {
    "provider": "stripe"
  }
}
